{
  "name": "bengbengsoft/beng-framework",
  "description": "bengbeng framework for yii2 ",
  "keywords": ["yii2", "framework", "bengbeng"],
  "type": "yii2-extension",
  "license": "MIT",
  "support": {
    "issues": "https://github.com/hahastein/beng-framework/issues",
    "source": "https://github.com/hahastein/beng-framework"
  },
  "authors": [
    {
      "name": "hahastein",
      "email": "146119@qq.com"
    }
  ],
  "minimum-stability": "dev",
  "require": {
    "php": ">=5.4.0",
    "yiisoft/yii2": "~2.0.6",
    "yiisoft/yii2-bootstrap": "~2.0.0"
  },
  "require-dev": {
    "yiisoft/yii2-codeception": "*",
    "codeception/base": "^2.2.3",
    "codeception/verify": "~0.3.1"
  },
  "autoload": {
    "psr-4": {
      "bengbeng\\framework\\": ""
    }
  },
  "extra": {
    "branch-alias": {
      "dev-1.0.master": "1.0.x-dev",
      "dev-master": "2.x-dev"
    },
    "asset-installer-paths": {
      "npm-asset-library": "vendor/npm",
      "bower-asset-library": "vendor/bower"
    }
  }
}
