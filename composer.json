{
  "name": "bengbengsoft/beng-framework",
  "description": "bengbeng framework for Yii2 ",
  "keywords": ["yii2", "framework", "bengbeng"],
  "type": "yii2-extension",
  "license": "MIT",
  "authors": [
    {
      "name": "hahastein",
      "email": "146119@qq.com"
    }
  ],
  "minimum-stability": "dev",
  "require": {
    "yiisoft/yii2": "~2.0.7",
    "yiisoft/yii2-imagine": "^2.0@dev",
    "yiisoft/yii2-jui": "~2.0.0@dev",
    "bower-asset/dropzone": "~4.0",
    "bower-asset/codemirror": "@stable",
    "bower-asset/summernote": "0.*",
    "overtrue/wechat": "~4.0",
    "jpush/jpush": "dev-master"
  },
  "require-dev": {
    "yiisoft/yii2-codeception": "~2.0"
  },
  "autoload": {
    "psr-4": {
      "bengbeng\\framework\\": ""
    }
  },
  "extra": {
    "asset-installer-paths": {
      "npm-asset-library": "vendor/npm",
      "bower-asset-library": "vendor/bower"
    }
  }
}
