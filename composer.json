{
  "name": "bengbengsoft/beng-framework",
  "description": "bengbeng framework for yii2 ",
  "keywords": ["yii2", "framework", "bengbeng"],
  "type": "yii2-extension",
  "license": "MIT",
  "authors": [
    {
      "name": "hahastein",
      "email": "146119@qq.com"
    }
  ],
  "minimum-stability": "dev",
  "require": {
    "yiisoft/yii2": "~2.0.7"
  },
  "require-dev": {
    "yiisoft/yii2-codeception": "~2.0"
  },
  "suggest": {
    "yiisoft/yii2-bootstrap": "Used when using layout 'left-menu', 'right-menu' or 'top-menu'"
  },
  "autoload": {
    "psr-4": {
      "bengbeng\\framework\\": ""
    }
  },
  "extra": {
    "branch-alias": {
      "dev-1.0.master": "1.0.x-dev",
      "dev-master": "2.x-dev"
    },
    "asset-installer-paths": {
      "npm-asset-library": "vendor/npm",
      "bower-asset-library": "vendor/bower"
    }
  }
}
